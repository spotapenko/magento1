<?xml version="1.0"?>
<!--
/**
 */
-->
<config>
    <modules>
        <Acme_Feedback>
            <version>1.0.0.0.1</version>
        </Acme_Feedback>
    </modules>
    <global>
        <models>
            <feedback>
                <class>Acme_Feedback_Model</class>
                <resourceModel>feedback_resource</resourceModel>
            </feedback>
            <feedback_resource>
                <class>Acme_Feedback_Model_Resource</class>
                <entities>
                    <feedbackpost>
                        <table>feedback_posts</table>
                    </feedbackpost>
                </entities>
            </feedback_resource>
        </models>
        <resources>
            <acme_feedback_setup>
                <setup>
                    <module>Acme_Feedback</module>
                    <class>Acme_Feedback_Model_Resource_Setup</class>
                </setup>
            </acme_feedback_setup>
        </resources>
        <events>
            <controller_front_init_routers>
                <observers>
                    <magentostudy_news>
                        <class>Acme_Feedback_Model_Observer</class>
                        <method>testListener</method>
                    </magentostudy_news>
                </observers>
            </controller_front_init_routers>
        </events>
    </global>
    <frontend>
        <routers>
            <acme_feedback>
                <use>standard</use>
                <args>
                    <module>Acme_Feedback</module>
                    <frontName>feedback</frontName>
                </args>
            </acme_feedback>
        </routers>
    </frontend>
</config>
