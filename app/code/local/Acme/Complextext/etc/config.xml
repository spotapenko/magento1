<?xml version="1.0"?>
<!--
/**
 */
-->
<config>
    <modules>
        <Acme_Complextext>
            <version>1.0.0.0.1</version>
        </Acme_Complextext>
    </modules>
    <frontend>
        <routers>
            <acme_complextext>
                <use>standard</use>
                <args>
                    <module>Acme_Complextext</module>
                    <frontName>complextext</frontName>
                </args>
            </acme_complextext>
        </routers>
        <layout>
            <updates>
                <acme_feedback>
                    <file>acme_complextext.xml</file>
                </acme_feedback>
            </updates>
        </layout>
    </frontend>
    <global>
        <models>
            <complextext>
                <class>Acme_Complextext_Model</class>
                <resourceModel>complextext_resource</resourceModel>
            </complextext>
            <complextext_resource>
                <class>Acme_Complextext_Model_Resource</class>
                <entities>
                    <eavfeedbackpost>
                        <table>eav_feedback_posts</table>
                    </eavfeedbackpost>
                </entities>
            </complextext_resource>
        </models>
        <resources>
            <acme_complextext_setup>
                <setup>
                    <module>Acme_Complextext</module>
                    <class>Acme_Complextext_Model_Resource_Setup</class>
                </setup>
            </acme_complextext_setup>
        </resources>
    </global>
</config>
